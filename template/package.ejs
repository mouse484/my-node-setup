{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "scripts": {
    <%_ if (type === 'node') { _%>
    <% let option = '' %>
    <% if (typescript) option += ' --ext ts --exec ts-node' %>
    <% if (dotenv) option += ' --require dotenv/config' %>
    "dev": "nodemon --watch src<%= option %> index",
    <%_ } _%>
    <%_ if (eslint) { _%>
    "lint": "eslint .",
    "fix": "eslint --fix .",
    <%_ } _%>
    "":""
  },
  "devDependencies": {
    <%_ if (eslint) { _%>
    "eslint": "^6.8.0",
    "eslint-config-mouse": "mouse484/eslint-config",
    <%_ } _%>
    <%_ if (prettier) { _%>
    "prettier": "^1.19.1",
    <%_ } _%>
    <%_ if (eslint && prettier){ _%>
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    <%_ } _%>
    <%_ if (type === 'node') { _%>
    "nodemon": "^2.0.2",
    <%_ } _%>
    <%_ if (typescript) { _%>
    "typescript": "^3.7.5",
    <%_ } _%>
    <%_ if (typescript && type === 'node') { _%>
    "@types/node": "^13.5.0",
    "ts-node": "^8.6.2",
    <%_ } _%>
    <%_ if (typescript && eslint) { _%>
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    <%_ } _%>
    <%_ if (dotenv) { _%>
    "dotenv": "^8.2.0",
    <%_ } _%>
    "": ""
  }
}


